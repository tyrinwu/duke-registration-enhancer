 {
  "name": "Duke Registration Enhancer",
  "description": "Chrome extension that enhances Dukehub's registration section.",
  "version": "0.1",
  "manifest_version": 2,
  "background": {
    "scripts": ["js/background.js"],
    "persistent": false
  },
  "permissions": [
    "activeTab"
  ],
  "web_accessible_resources":[
    "css/restyle.css"
  ],
  "options_ui": {
    "page":"options.html",
    "open_in_tab": true
  },

  "content_scripts": [ {
    "js": [ "js/vendor/jquery-3.2.1.min.js", "js/injectCss.js", "js/instantCollapse.js", "js/main.js" ],
    "css" : [ "css/restyle.css" ],
    "matches": ["*://ihprd.siss.duke.edu/psp/IHPRD01/EMPLOYEE/SA/c/DU_WORKCENTER.DU_WORKCENTER_09.GBL*"],
    "all_frames": true
  }]
}
